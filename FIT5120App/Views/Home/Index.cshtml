<!-- Start Schedule Area -->
<section class="schedule">
    <div class="container ">
        <div class="schedule-inner">
            <div class="row">
                <div class="col-lg-4 col-md-6 col-12 ">
                    <!-- single-schedule -->
                    <div class="single-schedule first">
                        <div class="inner bg-primary">
                            <div class="icon">
                                <i class="icofont-lungs"></i>
                            </div>
                            <div class="single-content">
                                @*<span>Lorem Amet</span>*@
                                <h4>Know about Occupational Lung Diseases(OLDs)</h4>
                                <p>Occupational Lung Diseases(OLDs) are lung conditions caused or made worse by the occupational exposure. Most of these diseases are preventable</p>
                                <a href="~/Home/ViewDiseases">LEARN MORE<i class="fa fa-long-arrow-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-12">
                    <!-- single-schedule -->
                    <div class="single-schedule middle">
                        <div class="inner bg-primary">
                            <div class="icon">
                                <i class="icofont-industries"></i>
                            </div>
                            <div class="single-content">
                                @*<span>Fusce Porttitor</span>*@
                                <h4>See the risks your workplace poses for you</h4>
                                <p>The damage you do to your lungs today cannot be reversed tomorrow. Make sure you know the risks related to your occupation.</p>
                                <a href="~/Home/Industries">LEARN MORE<i class="fa fa-long-arrow-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-12">
                    <!-- single-schedule -->
                    <div class="single-schedule middle">
                        <div class="inner bg-primary">
                            <div class="icon">
                                <i class="icofont-industries"></i>
                            </div>
                            <div class="single-content">
                                @*<span>Fusce Porttitor</span>*@
                                <h4>Enhance your knowledge about OLDs</h4>
                                <p>Take this quiz to find out more about OLDs. Know about the symptoms, causes and other risk factors.                                                     
                                                       </p>
                                <br />
                                <a href="~/Home/Quiz">Take the quiz<i class="fa fa-long-arrow-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
