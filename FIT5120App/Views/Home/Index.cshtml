<style>
    .gallery img {
        width: 100%;
    }

    .gallery {
        list-style: none;
        display: grid;
        grid-template-columns: 1fr 1fr 1fr 1fr 1fr 1fr;
        grid-column-gap: 5px;
    }

    li:nth-child(1) {
        grid-column: 3/span 2;
        grid-row: 7/span 1;
        z-index: 2;
    }

    li:nth-child(2) {
        grid-column: 1/span 8;
        grid-row: 1/ span 8;
    }

    .top-left {
        position: absolute;
        top: 200px;
        left: 30px;
        font-size: 30px;
        font-weight: 300;
        line-height: 1.4;
        width: 300px
    }

    .card {
        position: relative;
        display: inline-block;
    }

        .card .img-top {
            display: none;
            position: absolute;
            top: 0;
            left: 0;
            z-index: 99;
        }

        .card:hover .img-top {
            display: inline;
        }
</style>



